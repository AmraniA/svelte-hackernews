{
  "devDependencies": {
    "clean-css": "^4.0.12",
    "eslint": "^3.16.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-svelte": "^1.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2"
  },
  "name": "svelte-hackernews",
  "version": "1.0.0",
  "description": "Svelte Hacker News clone",
  "dependencies": {
    "compression": "^1.6.2",
    "eslint": "^3.16.1",
    "express": "^4.15.0",
    "firebase": "^3.7.0",
    "lru-cache": "^4.0.2",
    "roadtrip": "^0.4.1"
  },
  "scripts": {
    "build:all": "npm run build:server && npm run build:client && npm run build:sw",
    "build:server": "node build",
    "build:client": "rollup -c client/rollup.config.js",
    "build:sw": "rollup -c service-worker/rollup.config.js",
    "deploy": "npm run build:all && now && now alias",
    "dev:client": "rollup -c client/rollup.config.js -w --environment DEVELOPMENT:true",
    "dev:sw": "rollup -c service-worker/rollup.config.js -w --environment DEVELOPMENT:true",
    "dev": "npm run build:server && npm run dev:client & npm run dev:sw & node server",
    "start": "node server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sveltejs/svelte-hackernews.git"
  },
  "author": "Rich Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sveltejs/svelte-hackernews/issues"
  },
  "homepage": "https://github.com/sveltejs/svelte-hackernews#readme",
  "now": {
    "alias": "svelte-hn.now.sh",
    "files": [
      "public",
      "server",
      "shared"
    ]
  }
}
